{
    "name": "dawdistancia/banco_soap_hipoteca",
    "description": "Aplicación de operación bancaria",
    "type": "project",
    "require": {
        "fakerphp/faker": "^1.23",
        "eftec/bladeone": "^4.11",
        "vlucas/phpdotenv": "^5.6"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "TipoCambio\\": "soapdivisas/src/",
            "Hipoteca\\": "soaphipoteca/src"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "require-dev": {
        "phpro/soap-client": "3.3.0",
        "symfony/http-client": "^6.4",
        "nyholm/psr7": "^1.8",
        "laminas/laminas-soap": "^2.14"
    }
}
